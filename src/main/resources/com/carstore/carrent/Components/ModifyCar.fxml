<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white; -fx-background-radius: 16px;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.carstore.carrent.Components.ModifyCarController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="16.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox prefHeight="58.0" prefWidth="568.0">
               <children>
                  <Label text="Manufacturer">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <TextField fx:id="manufacturerField" alignment="BOTTOM_LEFT">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
               </children>
            </VBox>
            <VBox prefHeight="58.0" prefWidth="568.0">
               <children>
                  <Label text="Model">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <TextField fx:id="modelField" alignment="BOTTOM_LEFT">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
               </children>
            </VBox>
            <VBox prefHeight="58.0" prefWidth="568.0">
               <children>
                  <Label text="Color">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <TextField fx:id="colorField" alignment="BOTTOM_LEFT">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
               </children>
            </VBox>
            <VBox prefHeight="58.0" prefWidth="568.0">
               <children>
                  <Label text="Year Of Manufacturer">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <TextField fx:id="yearOfManufacturerField" alignment="BOTTOM_LEFT">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
               </children>
            </VBox>
            <VBox prefHeight="58.0" prefWidth="568.0">
               <children>
                  <Label text="Rent Price">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <TextField fx:id="rentPriceField" alignment="BOTTOM_LEFT">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="8.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="24.0">
                     <children>
                        <Button fx:id="modifyButton" mnemonicParsing="false" onAction="#updateCar" text="Modify Car">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#deleteCar" text="Delete Car">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
                  <Button fx:id="closeWindow" mnemonicParsing="false" onAction="#closeWindow" prefHeight="44.0" prefWidth="270.0" text="Close">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</AnchorPane>
