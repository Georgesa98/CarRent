<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="125.0" prefWidth="300.0" style="-fx-background-color: white; -fx-background-radius: 10px;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.carstore.carrent.Components.RentCardController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="4.0">
         <children>
            <Label text="Name Of Client:" />
            <Label fx:id="clientName" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="4.0">
         <children>
            <Label text="Car Model:" />
            <Label fx:id="carModel" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="4.0">
         <children>
            <Label text="Rent End Date:" />
            <Label fx:id="rentEndDate" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="4.0">
         <children>
            <Label text="Payment:" />
            <Label fx:id="payment" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
